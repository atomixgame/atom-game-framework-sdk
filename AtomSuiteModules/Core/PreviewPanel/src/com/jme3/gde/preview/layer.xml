<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE filesystem PUBLIC "-//NetBeans//DTD Filesystem 1.2//EN" "http://www.netbeans.org/dtds/filesystem-1_2.dtd">
<filesystem>
    <folder name="Navigator">
        <folder name="Panels">
            <!-- Sudirectory structure below represents content type in mime type syntax.
                 Replace with content type your navigator view will show  -->
            <folder name="image">
                <folder name="x-jmetexture">
                    <file name="com-jme3-gde-preview-BasicNavPanelImpl.instance"/>
                </folder>
                <folder name="png">
                    <file name="com-jme3-gde-preview-BasicNavPanelImpl.instance"/>
                </folder>
                <folder name="jpeg">
                    <file name="com-jme3-gde-preview-BasicNavPanelImpl.instance"/>
                </folder>
                <folder name="gif">
                    <file name="com-jme3-gde-preview-BasicNavPanelImpl.instance"/>
                </folder>
                <folder name="psd">
                    <file name="com-jme3-gde-preview-BasicNavPanelImpl.instance"/>
                </folder>
            </folder>
            <folder name="text">
                <folder name="ogrexml+xml">
                    <file name="com-jme3-gde-preview-BasicNavPanelImpl.instance"/>
                </folder>
            </folder>
        </folder>
    </folder>
    <folder name="Services">
        <folder name="MIMEResolver">
            <file name="PSDImageResolver.xml" url="PSDImageResolver.xml">
                <attr name="displayName" stringvalue="com.jme3.gde.preview.PSDImageFile"/>
            </file>
        </folder>
    </folder>
</filesystem>